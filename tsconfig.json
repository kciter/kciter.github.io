{
  "extends": "astro/tsconfigs/strict",
  "include": ["src/**/*", "src/env.d.ts"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@materials/*": ["./src/materials/*"],
      "@assets/*": ["./src/assets/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@utils/*": ["./src/utils/*"]
    }
  }
}
